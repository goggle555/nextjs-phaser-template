  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "22.10.1",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.2",
    "husky": "9.1.7",
    "lint-staged": "15.2.10",
    "typescript": "5.7.2"
  }
}
